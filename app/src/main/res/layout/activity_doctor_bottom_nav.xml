<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.BottomNavActivity">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/doctor_frame_container"
        android:layout_above="@+id/doctor_bottomNav"/>

    <com.ismaeldivita.chipnavigation.ChipNavigationBar
        android:id="@+id/doctor_bottomNav"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@drawable/bg_bottom_nav"
        android:paddingTop="@dimen/_10sdp"
        android:paddingBottom="@dimen/_10sdp"
        app:cnb_unselectedColor="@color/white"
        android:elevation="@dimen/_13sdp"
        app:cnb_radius="8dp"
        android:layout_marginBottom="@dimen/_8sdp"
        android:layout_marginHorizontal="@dimen/_8sdp"
        app:cnb_menuResource="@menu/doctor_bottom_nav_menu"/>

</RelativeLayout>